	SET BOARD_STATUS = 'N'
	WHERE BOARD_ID = #{boardId}
</update>

<!-- 나의 쿠폰 리스트 조회 -->
<select id="selectCouponList" parameterType="_int" resultMap="SelectCouponResultSet">
